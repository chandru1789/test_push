pipeline {
    agent none
    stages {
        stage('Calling Upstream Jobs') {
            parallel {
                stage('UpStreamJob1') {
                   
                    steps {
                        build job: 'UpStreamJob'
                    }
                    post {
                        always {
                            echo "UpStreamJob1 Completed Successfully"
                        }
                    }
                }
                stage('UpStreamJob2') {
                   
                    steps {
                        build job: 'UpStreamJob'
                    }
                    post {
                        always {
                           echo "UpStreamJob2 Completed Successfully"
                        }
                    }
                }
            }
        }
    }
}