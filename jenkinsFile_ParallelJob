pipeline {
  agent any
  parallel firstBranch: {
        // do something
	stage ('UpStreamJob1') {
	  echo "UpStreamJob1 execution"
      steps {
        build job: 'UpStreamJob'
      }
    }
    }, secondBranch: {
        // do something else
	stagetage ('UpStreamJob2') {
	  echo "UpStreamJob2 execution"
      steps {
        build job: 'UpStreamJob2'
      }
    }
    },
    failFast: true
}